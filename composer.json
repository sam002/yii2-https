    "source": "https://github.com/sam002/yii2-https"
  },
  "authors": [
    {
      "name": "Semen Dubina",
      "email": "yii2-https@sam002.net",
      "homepage": "http://sam002.net/project/yii2-https",
      "role": "Developer"
    }
  ],

  "require": {
    "yiisoft/yii2": "2.0.*",
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },

  "autoload": {
    "psr-4": {
      "sam002\\https\\": "src"
    }
  }
